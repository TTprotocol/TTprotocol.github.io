		"@testing-library/user-event": "^13.5.0",
		"gh-pages": "^6.1.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-scripts": "5.0.1",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
